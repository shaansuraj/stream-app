{
  "name": "streaming-app",
  "version": "1.0.0",
  "description": "a sample streaming app with live chat as divine talk assesment",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src, test}/**/*.ts\"",
